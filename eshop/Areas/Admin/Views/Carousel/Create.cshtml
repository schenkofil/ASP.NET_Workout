@using eshop.Areas.Admin.Models 
@model Carousel
@{
    ViewData["Title"] = "Carousel Create";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

@{ViewBag.Action = "Create";}

<partial name="_Form" model="Model">

    @section Scripts {

        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        
        <environment include="Development">
            <script src="~/js/Validation/ContentTypeValidation.js"></script>
        </environment>
        <environment exclude="Development">
            <script src="~/js/Validation/ContentTypeValidation.min.js"></script>
        </environment>
    }
